{
  "expo": {
    "name": "Safecheck",
    "slug": "safe-check-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo.png",
    "splash": {
      "image": "./assets/app_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safcheck.safecheckapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.safcheck.safecheckapp",
      "permissions": ["CAMERA"],
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
